{
    "manifest_version": 3,
    "name": "Pass",
    "description": "UI extension based on unix-pass tool.",
    "version": "0.0.0",
    "icons": {
        "16": "icons/icon-16.png",
        "48": "icons/icon-48.png",
        "128": "icons/icon-128.png",
        "256": "icons/icon-256.png"
    },
    "action": {
        "default_popup": "index.html",
        "default_icon": {
            "16": "icons/icon-16.png",
            "24": "icons/icon-24.png",
            "32": "icons/icon-32.png"
        }
    },
    "permissions": [
        "storage",
        "activeTab",
        "scripting"
    ],
    "host_permissions": [
        "https://*/*"
    ]
}